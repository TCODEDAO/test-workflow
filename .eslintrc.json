{
  "env": {
    "browser": true,
    "es2021": true,
    "commonjs": true,
    "jquery": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "semi": ["error", "never"],
    "comma-dangle": [
      "error",
      {
        "arrays": "never",
        "objects": "never",
        "imports": "never",
        "exports": "never",
        "functions": "never"
      }
    ],
    "linebreak-style": ["error", "windows"],
    "no-undef": "off"
  }
}
